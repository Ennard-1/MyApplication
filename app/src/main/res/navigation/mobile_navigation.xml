<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_videos">

    <fragment
        android:id="@+id/navigation_videos"
        android:name="com.example.myapplication.ui.videos.VideosFragment"
        android:label="Videos"
        tools:layout="@layout/fragment_videos" />

    <fragment
        android:id="@+id/navigation_playlists"
        android:name="com.example.myapplication.ui.playlists.PlaylistsFragment"
        android:label="Playlists"
        tools:layout="@layout/fragment_playlists" />

    <fragment
        android:id="@+id/navigation_conta"
        android:name="com.example.myapplication.ui.conta.ContaFragment"
        android:label="Conta"
        tools:layout="@layout/fragment_conta" />


</navigation>